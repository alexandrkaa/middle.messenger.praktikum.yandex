  "compilerOptions": {
    "target": "es2019",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "declaration": true,
    "module": "commonjs",
    "strictNullChecks": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "paths": {
