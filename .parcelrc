{
  "extends": "@parcel/config-default",
  "transformers": {
    "*.{ts,tsx}": [
      "@parcel/transformer-typescript-tsc"
    ],
    "*.hbs": [
      "@anikin/parcel-transformer-handlebars"
    ],
    "*.scss": [
      "@parcel/transformer-sass"
    ]
  },
  "validators": {
    "*.{ts,tsx}": [
      "@parcel/validator-typescript"
    ]
  }
}
